!!! 5
%html
  %head
    %title= title
    = csrf_meta_tag
    = render "layouts/stylesheets"

  %body
    .container
      = render "layouts/header"

      - flash.each do |key, value|
        %div{:class => "flash #{key}"}= value

      %section= yield

      = render "layouts/footer"
      = debug(params) if Rails.env.development?
